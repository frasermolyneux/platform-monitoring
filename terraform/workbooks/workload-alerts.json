        {
            "type": 3,
            "content": {
                "version": "KqlItem/1.0",
                "query": "resources\r\n| where type =~ 'microsoft.insights/activitylogalerts'\r\n     or type =~ 'microsoft.alertsmanagement/smartdetectoralertrules'\r\n     or type =~ 'microsoft.insights/scheduledqueryrules'\r\n     or type =~ 'microsoft.insights/alertrules'\r\n     or type =~ 'microsoft.insights/metricalerts'\r\n| join kind=leftouter(\r\n        alertsmanagementresources \r\n        | extend MonitorCondition=tostring(properties.essentials.monitorCondition)\r\n        | where todatetime(properties.essentials.startDateTime) {TimeRange}\r\n    ) on name\r\n| extend Workload = iff(isnotempty(tags.Workload), tostring(tags.Workload), \"Unknown\")\r\n| extend Environment = iff(tostring(tags.Environment) != \"\", tostring(tags.Environment), \"Unknown\")\r\n| summarize Total = countif(MonitorCondition == \"Fired\") by Workload, Environment\r\n| extend HasAlerts = iif(Total > 0, \"true\", \"false\")\r\n| order by Total desc, Environment desc, Workload asc \r\n",
                "size": 0,
                "title": "Workload Health - 'MonitorCondition == Fired'",
                "exportedParameters": [
                    {
                        "fieldName": "Workload",
                        "parameterName": "Workload",
                        "parameterType": 1
                    },
                    {
                        "fieldName": "Environment",
                        "parameterName": "Environment",
                        "parameterType": 1
                    },
                    {
